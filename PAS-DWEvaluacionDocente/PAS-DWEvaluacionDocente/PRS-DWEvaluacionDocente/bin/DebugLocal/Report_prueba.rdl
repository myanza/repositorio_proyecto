  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS_Hechos_Semestral</DataSourceName>
        <CommandText>SELECT        PERIODO.PERIODO_COMPLETO, DOCENTE.SIGLA_FACULTAD AS DOC_FACULTAD, DOCENTE.DEPARTAMENTO AS DOC_DEPARTAMENTO, COUNT(DOCENTE.NOMBRE_COMPLETO) AS CANTIDAD_DOCENTES
FROM            HECHO_FINAL_SEMESTRAL.HECHO_FINAL_SEMESTRAL, DW_ACADEMICO_LABOR.DOCENTE, DW_ACADEMICO_LABOR.PERIODO, HECHO_FINAL_SEMESTRAL.SEGUIMIENTO
WHERE        (HECHO_FINAL_SEMESTRAL.DOCENTE_ID = DOCENTE.DOCENTE_ID) AND (HECHO_FINAL_SEMESTRAL.PERIODO_ID = PERIODO.PERIODO_ID) AND 
                         (HECHO_FINAL_SEMESTRAL.SEGUIMIENTO_ID = SEGUIMIENTO.SEGUIMIENTO_PK) AND (PERIODO.PERIODO_COMPLETO IN ('2017-2', '2017-1', '2016-2', '2016-1', '2015-2')) AND (SEGUIMIENTO.SEGUIMIENTO = 'Si')
GROUP BY PERIODO.PERIODO_COMPLETO, DOCENTE.SIGLA_FACULTAD, DOCENTE.DEPARTAMENTO
ORDER BY PERIODO.PERIODO_COMPLETO, DOC_FACULTAD, DOC_DEPARTAMENTO</CommandText>
      </Query>
      <Fields>
        <Field Name="PERIODO_COMPLETO">
          <DataField>PERIODO_COMPLETO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOC_FACULTAD">
          <DataField>DOC_FACULTAD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOC_DEPARTAMENTO">
          <DataField>DOC_DEPARTAMENTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CANTIDAD_DOCENTES">
          <DataField>CANTIDAD_DOCENTES</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
