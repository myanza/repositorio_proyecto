        MUNNACIMIENTO.CODIGO "CODIGO_MUNICIPIO_NAC",
        MUNPROCEDENCIA.CODIGO "CODIGO_MUNICIPIO_PRO"        
FROM TERCEROS JOIN DOCENTES ON (TERCEROS.OID = DOCENTES.TERCERO) 
      LEFT JOIN 
     MUNICIPIOS "MUNNACIMIENTO" ON (TERCEROS.MUNICIPIONACIMIENTO = MUNNACIMIENTO.OID)
     LEFT JOIN 
     MUNICIPIOS "MUNPROCEDENCIA" ON (TERCEROS.MUNICIPIOPROCEDENCIA = MUNPROCEDENCIA.OID)
ORDER BY DOCENTES.UNIDAD</property>
