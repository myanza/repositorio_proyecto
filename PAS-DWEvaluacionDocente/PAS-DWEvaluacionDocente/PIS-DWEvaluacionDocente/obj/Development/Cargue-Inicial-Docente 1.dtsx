                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
        DOCENTES.UNIDAD  "OID_DEPARTAMENTO_DOCENTE",
        TERCEROS.PRIMERNOMBRE,
        TERCEROS.SEGUNDONOMBRE,
        TERCEROS.PRIMERAPELLIDO,
        TERCEROS.SEGUNDOAPELLIDO,
        TERCEROS.IDENTIFICACION,
        TERCEROS.GENERO,
        TERCEROS.EMAIL,
