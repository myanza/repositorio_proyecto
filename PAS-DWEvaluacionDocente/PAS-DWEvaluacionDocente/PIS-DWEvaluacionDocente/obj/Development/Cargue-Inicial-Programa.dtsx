programas.sniesmunicipio,
initcap(municipios.descripcion) as SEDE,
initcap(nucleoconocimiento.descripcion) as NUCLEO
from programas,facultades,areasconocimiento,nucleoconocimiento,fasesacreditacion,municipios
where programas.facultad = facultades.oid(+)
and programas.areaconocimiento = areasconocimiento.oid(+)
and programas.nucleobasicoconocimiento = nucleoconocimiento.oid(+)
and programas.faseacreditacion = fasesacreditacion.oid(+)
