                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select initcap(programas.nombre) as NOMBREPROGRAMA,
initcap(facultades.nombre) as NOMBREFACULTAD,
facultades.sigla as SIGLA,
programas.jornada,
programas.duracion,
programas.metodologia,
programas.regionalizacion,
programas.prefijocodigo,
programas.admision,
initcap(fasesacreditacion.descripcion) as FASEACREDITACION,
