                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
PROGRAMAS.NOMBRE, 
FACULTADES.SIGLA SIGLA_FACULTAD, 
JORNADA, 
DURACION , 
METODOLOGIA, 
REGIONALIZACION , 
PREFIJOCODIGO , 
ADMISION,  
FASESACREDITACION.DESCRIPCION AS FASE_ACREDITACION, 
AREASCONOCIMIENTO.DESCRIPCION AREA_CONOCIMIENTO, 
SNIESPROGRAMA, 
SNIESMUNICIPIO, 
MUNICIPIOS.DESCRIPCION SEDE, 
NUCLEOCONOCIMIENTO.DESCRIPCION NUCLEO_BASICO
FROM PROGRAMAS JOIN MUNICIPIOS ON (PROGRAMAS.SEDE = MUNICIPIOS.OID)
LEFT JOIN FACULTADES ON (FACULTADES.OID=PROGRAMAS.FACULTAD)
LEFT JOIN FASESACREDITACION ON (PROGRAMAS.FASEACREDITACION=FASESACREDITACION.OID)
LEFT JOIN AREASCONOCIMIENTO ON (PROGRAMAS.AREACONOCIMIENTO=AREASCONOCIMIENTO.OID)
LEFT JOIN NUCLEOCONOCIMIENTO ON (PROGRAMAS.NUCLEOBASICOCONOCIMIENTO=NUCLEOCONOCIMIENTO.OID)</property>
