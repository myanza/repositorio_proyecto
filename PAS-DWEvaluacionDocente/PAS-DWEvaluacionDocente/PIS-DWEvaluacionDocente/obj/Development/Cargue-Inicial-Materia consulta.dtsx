                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
    MATERIAS.OID MATERIA_OID,
    MATERIAS.DESCRIPCION MATERIA,
    GRUPOSCLASE.SECCION NOMBRE_CURSO,
    MATERIAS.CODIGO CODIGO_MATERIA,
    PENSUMS.DESCRIPCION NOMBRE_PENSUM,
    RELACIONESMATERIAS.MATERIA MATERIA_RELACIONADA,
    NOTASCONFIGURACION.CUANTITATIVA INDICADOR_NOTA,
    MATERIAS.LABORATORIO TIPO_MATERIA,
    GRUPOSCLASE.PERIOCIDAD,
    MATERIAS.PROGRAMA  
FROM MATERIAS JOIN GRUPOSCLASE ON (MATERIAS.OID=GRUPOSCLASE.MATERIA) 
LEFT JOIN PENSUMMATERIAS ON (PENSUMMATERIAS.MATERIA=MATERIAS.OID)
LEFT JOIN PENSUMS ON (PENSUMS.OID=PENSUMMATERIAS.PENSUM)
LEFT JOIN RELACIONESMATERIAS ON (MATERIAS.OID=RELACIONESMATERIAS.RELACIONADA)
LEFT JOIN NOTASCONFIGURACION ON (NOTASCONFIGURACION.GRUPO=GRUPOSCLASE.OID)
ORDER BY MATERIAS.OID</property>
